# Backend Environment Variables Example
# Copy this file to .env for local development

PORT=3001

# --- Follow Up Boss Configuration ---
FUB_API_KEY=your_fub_api_key_here
FUB_X_SYSTEM=your_x_system_header
FUB_X_SYSTEM_KEY=your_x_system_key_header
FUB_EUGENIA_LINK_CUSTOM_FIELD_NAME=your_fub_custom_field_api_name_for_eugenia_link
FUB_USER_ID_FOR_AI=your_fub_numeric_user_id_for_eugenia_texts

# --- Gemini Configuration ---
GEMINI_API_KEY=your_gemini_api_key_here
USER_AGENCY_NAME=Your Awesome Realty

# --- Twilio Configuration ---
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_FROM_NUMBER=your_twilio_phone_number_in_e164_format_eg_+1234567890

# --- Airtable Configuration (for later) ---
AIRTABLE_API_KEY=your_actual_airtable_api_key_if_you_have_one
AIRTABLE_BASE_ID=your_airtable_base_id_e.g_appXXXXXXXXXXXXXX
AIRTABLE_LEADS_TABLE_NAME=Leads
AIRTABLE_MESSAGES_TABLE_NAME=Messages

# --- Authentication Configuration ---
ADMIN_EMAIL=admin@eugenia.app
ADMIN_PASSWORD_HASH=your_bcrypt_hashed_admin_password
JWT_SECRET=your_long_random_jwt_secret_64_chars_or_more

# --- Application Configuration ---
APP_DOMAIN=localhost:3000